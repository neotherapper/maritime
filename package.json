{
  "name": "maritime",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "e2e": "pnpm e2e:run",
    "e2e:run": "pnpm playwright test",
    "e2e:ui": "pnpm playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook",
    "test:storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm build-storybook --quiet\" \"wait-on tcp:6006 && pnpm test:storybook\""
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@msw/playwright": "^0.3.1",
    "@playwright/test": "^1.53.1",
    "@storybook/addon-a11y": "^9.0.13",
    "@storybook/addon-docs": "^9.0.13",
    "@storybook/addon-vitest": "^9.0.13",
    "@storybook/react": "^9.0.13",
    "@storybook/react-vite": "^9.0.13",
    "@storybook/test-runner": "^0.23.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.0.13",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "msw-storybook-addon": "^2.0.5",
    "postcss": "^8.5.6",
    "storybook": "^9.0.13",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.3"
  }
}
